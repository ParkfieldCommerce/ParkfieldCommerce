<div class="ContactPopUp">
	<div class="ContactPopUp__overlay">
		<div class="ContactPopUp__form">
			<div id="CloseContactPopUp" class="ContactPopUp__close">
				<%= partial './partials/global/svg-icons', locals:{icon:'close'} %>
			</div>
			<h1><%= dato.contact_form.heading %></h1>
			<p><%= dato.contact_form.form_text %></p>
			<form id="form96" name="form96" class="wufoo topLabel page" accept-charset="UTF-8" autocomplete="off" enctype="multipart/form-data" method="post" novalidate action="https://shopifybuilder.wufoo.eu/forms/kmjfzql1wfq68r/#public">
			<ul>
			<div class="ContactPopUp__formTopRow">
					<li id="foli4" class="notranslate      ">
					<div class="ContactPopUp__input">
					<input id="Field4" name="Field4" type="text" class="field text medium" value="" maxlength="255" tabindex="1" onkeyup=""       placeholder="" />
					<label class="desc" id="title4" for="Field4">
					Full name
					</label>
					</div>
					</li>
					<li id="foli9" class="notranslate      ">
					<div class="ContactPopUp__input">
					<input id="Field9" name="Field9" type="email" spellcheck="false" class="field text medium" value="" maxlength="255" tabindex="4"       placeholder="" />
					<label class="desc" id="title9" for="Field9">
					Email
					</label>
					</div>
					</li>
					<li id="foli13" class="notranslate      ">
					<div class="ContactPopUp__input">
					<input id="Field13" name="Field13" type="text" class="field text nospin medium"  value="" tabindex="2" onkeyup="" placeholder=""       />
					<label class="desc " id="title13" for="Field13">
					Phone
					</label>
					</div>
					</li>
					<li id="foli10" class="notranslate       ">
					<div class="ContactPopUp__input">
					<input id="Field10" name="Field10" type="url" class="field text medium" value="" maxlength="255" tabindex="5" placeholder=""       />
					<label class="desc" id="title10" for="Field10">
					Website URL (Optional)
					</label>
					</div>
					</li>
					<li id="foli7" class="hiddenSelect">
					<div class="ContactPopUp__input">
					<select id="Field7" name="Field7" class="field select medium"       tabindex="3" >
					<option
					value="" selected="selected">
					<span class="notranslate"></span>
					</option>
					<option 
					value="&lt; $10,000" >
					<span class="notranslate">< $10,000</span>
					</option>
					<option
					value="$10,000 - $15,000" >
					<span class="notranslate">$10,000 - $15,000</span>
					</option>
					<option
					value="$15,000 - $20,000" >
					<span class="notranslate">$15,000 - $20,000</span>
					</option>
					<option
					value="&gt; $20,000" >
					<span class="notranslate">> $20,000</span>
					</option>
					</select>
					</div>
					</li>
					<li class="dropdownMenu">
					<div class="ContactPopUp__input dropdownMenuWrap">
						<input type="text" id="select" value="" readonly>
						<div class="chevron"><%= partial './partials/global/svg-icons', locals:{icon:'dropdown'} %></div>
						<label class="desc notranslate" id="title7" for="select">
					Budget
					</label>
						<ul class="ContactPopUp__dropdown">
							<li class="dataSelector" data-value="&lt; $10,000">< $10,000</li>
							<li class="dataSelector" data-value="$10,000 - $15,000">$10,000 - $15,000</li>
							<li class="dataSelector" data-value="$15,000 - $20,000">$15,000 - $20,000</li>
							<li class="dataSelector" data-value="&gt; $20,000">> $20,000</li>
						</ul>
					</div>
					</li>
			</div>
			<div class="ContactPopUp__textarea">
				<li id="foli15"
				class="notranslate      ">
				<div class="ContactPopUp__input">
				<textarea id="Field15"
				name="Field15"
				class="field textarea medium"
				spellcheck="true"
				rows="1" cols="50"

				tabindex="6"
				onkeyup=""
				placeholder=""
				       ></textarea>
				<label class="desc" id="title15" for="Field15">
				Tell us about your project
				</label>
				</div>
			</div>
			<div class="ContactPopUp__submit">
				<input id="saveForm" name="saveForm" class="btTxt submit" type="submit" value="<%= dato.contact_form.submit_button_text %>"/>
				<%= image_tag dato.contact_form.form_image.url, alt: dato.contact_form.form_image.alt, class:"ContactPopUp__image" %>
			</div>
			

			<li class="hide">
			<label for="comment">Do Not Fill This Out</label>
			<textarea name="comment" id="comment" rows="1" cols="1"></textarea>
			<input type="hidden" id="idstamp" name="idstamp" value="B3CCQjcvEz00I+Ktnxzv1QoH+sO8IelY0gkMBi8kT1M=" />
			</li>
			</ul>
			<a class="ContactPopUp__call" href="<%= dato.contact_form.phone_number_link %>"><%= dato.contact_form.phone_number_link_text %></a>
			</form>
		</div>
	</div>
</div>