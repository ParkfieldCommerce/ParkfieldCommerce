<% emailTitle = dato.case_study_email_form.title %>
<% emailPhone = dato.case_study_email_form.phone %>
<% buttonTitle = dato.case_study_email_form.button_title %>

<div class="container m">
	<div class="row space-90 mobile-space-60 CaseStudy__form__container--1">
			<div class="CaseStudy CaseStudy__form__container--2 large-12 medium-10 small-12">
				<div id="mc_embed_signup">
					<form action="https://nyc.us16.list-manage.com/subscribe/post?u=f127562a91d5c95b40b01c49e&amp;id=093cc5a009" method="post" id="mc-embedded-subscribe-form" name="mc-embedded-subscribe-form" class="validate" target="_blank" novalidate>
						<h3> <%= emailTitle %> </h3>
						<div class="CaseStudy__form__container--3" id="mc_embed_signup_scroll">
							<input type="email" value="" placeholder="Enter your email address" name="EMAIL" id="mce-EMAIL" aria-label="" autocorrect="off" autocapitalize="off" required>
							<div style="position: absolute; left: -5000px;" aria-hidden="true"><input type="text" name="b_f127562a91d5c95b40b01c49e_093cc5a009" tabindex="-1" value=""></div>
							<button id="mc-embedded-subscribe" type="submit" class="Btn--animatedLeft--purple"><span><%= buttonTitle %></span></button>
						</div>
						<a href="tel:<%= emailPhone %>" class="hidden-desktop Btn--animatedLeft--purple"><span>Call <%= emailPhone %></span></a>
					</form>
				</div>
			</div>
	</div>
</div>
<script type="text/javascript" src="../../javascripts/vendor/ajaxchimp.min.js"></script>