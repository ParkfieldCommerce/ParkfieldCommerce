<!doctype html>
<html>
  <head>
    <meta charset="utf-8">
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <meta name="viewport"
          content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta name="google-site-verification" content="jmU8inKAKIDdH-yolQVFHtPIGtoFCZiJ5pE-5u6TY5M" />
    <%= yield_content :head %>
    <%= dato_favicon_meta_tags %>
    <meta name="description" content="<%= current_page.data.description %>"/>
    <!-- Use the title from a page's frontmatter if it has one -->
    <title><%= current_page.data.title %></title>
    <%= stylesheet_link_tag "app" %>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>
    <script src="//cdnjs.cloudflare.com/ajax/libs/velocity/2.0.1/velocity.min.js"></script>
    <%= javascript_include_tag "/javascripts/vendor/slick.min" %>
    <%= partial './partials/scripts/google-listing' %>
    <%= partial './partials/scripts/google-tracking' %>
    <%= partial './partials/scripts/hotjar' %>
    <%= partial './partials/scripts/mailchimp' %>
  </head>
  <body class="<%= current_page.data.pageClass %>">
    <div class="container m">
    <%= partial './partials/header/header' %>
    </div>
    <%= yield %>
    <%= javascript_include_tag "app" %>
    <%= partial './partials/scripts/adroll' %>
  </body>
</html>
